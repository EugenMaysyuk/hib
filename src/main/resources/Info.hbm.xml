<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

  <class name="entities.Info" table="info">
    <id name="infoId" column="info_id" type="int">
      <generator class="native"></generator>
    </id>
    <property name="bankId" column="bank_id"/>
    <property name="text1" column="text1"/>
    <property name="text2" column="text2"/>
    <property name="text3" column="text3"/>
  </class>
</hibernate-mapping>